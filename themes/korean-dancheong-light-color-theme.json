{
  "name": "korean-dancheong-light-vivid",
  "type": "light",
  "semanticHighlighting": true,
  "colors": {
    "foreground": "#1A1A1A",
    "focusBorder": "#00B8B2A0",
    "selection.background": "#E1F6F1",

    "editor.background": "#FCFAF6",
    "editor.foreground": "#1A1A1A",
    "editorLineNumber.foreground": "#9AA1A6",
    "editorLineNumber.activeForeground": "#004DD6",
    "editorCursor.foreground": "#004DD6",
    "editor.selectionBackground": "#E1F6F1",
    "editor.selectionHighlightBackground": "#F8F3CC",
    "editor.wordHighlightBackground": "#FFF7D6",
    "editor.wordHighlightStrongBackground": "#FFEEAA",
    "editor.lineHighlightBackground": "#F5F1E8",
    "editor.inactiveSelectionBackground": "#EAF7F3",
    "editor.findMatchHighlightBackground": "#FFF3C4",
    "editorLink.activeForeground": "#004DD6",

    "editorGutter.addedBackground": "#4C6057",
    "editorGutter.modifiedBackground": "#004DD6",
    "editorGutter.deletedBackground": "#DF3C3C",

    "editorBracketMatch.background": "#EAF0FF",
    "editorBracketMatch.border": "#24478F",

    "editorIndentGuide.background": "#E6E1D6",
    "editorIndentGuide.activeBackground": "#D7D0C1",
    "editorWhitespace.foreground": "#DDD6C9",
    "editorHoverWidget.background": "#FFFDF7",
    "editorHoverWidget.border": "#E2DDD1",

    "activityBar.background": "#F7F4EC",
    "activityBar.foreground": "#1A1A1A",
    "activityBar.inactiveForeground": "#7C868E",
    "activityBarBadge.background": "#00B8B2",
    "activityBarBadge.foreground": "#FFFFFF",

    "sideBar.background": "#FAFAF8",
    "sideBar.foreground": "#2E3238",
    "sideBarSectionHeader.background": "#F2EFE8",
    "sideBarTitle.foreground": "#2E3238",

    "tab.activeBackground": "#FCFAF6",
    "tab.inactiveBackground": "#EFECE6",
    "tab.activeForeground": "#1A1A1A",
    "tab.inactiveForeground": "#6B737B",
    "tab.border": "#E2DDD1",
    "tab.activeBorder": "#00B8B2",

    "titleBar.activeBackground": "#F7F4EC",
    "titleBar.activeForeground": "#2B2F34",
    "titleBar.inactiveForeground": "#7C868E",

    "statusBar.background": "#F7F4EC",
    "statusBar.foreground": "#2B2F34",
    "statusBar.noFolderBackground": "#EDE8DC",
    "statusBar.debuggingBackground": "#D99A00",
    "statusBar.debuggingForeground": "#FFFFFF",

    "panel.background": "#FDFBF6",
    "panel.border": "#E2DDD1",

    "list.activeSelectionBackground": "#E3F5EF",
    "list.activeSelectionForeground": "#1A1A1A",
    "list.activeSelectionIconForeground": "#1A1A1A",
    "list.inactiveSelectionBackground": "#F0F9F6",
    "list.hoverBackground": "#F1F9F5",
    "list.errorForeground": "#DF3C3C",
    "list.warningForeground": "#D99A00",
    "list.focusBackground": "#D9F2EC",

    "breadcrumb.foreground": "#7C868E",
    "breadcrumb.focusForeground": "#24478F",

    "badge.background": "#00B8B2",
    "badge.foreground": "#FFFFFF",

    "button.background": "#00B8B2",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#00A19E",

    "notificationCenterHeader.background": "#F7F4EC",
    "notifications.background": "#FFFDF7",
    "notifications.foreground": "#2B2F34",

    "minimap.findMatchHighlight": "#FFF3C4",
    "minimap.selectionHighlight": "#CFE6E1",
    "minimap.errorHighlight": "#DF3C3C",
    "minimap.warningHighlight": "#D99A00",

    "editorWidget.background": "#FFFDF7",
    "editorWidget.border": "#E2DDD1",
    "peekViewEditor.background": "#FFFDF7",
    "peekViewResult.background": "#FAFAF8",

    "diffEditor.insertedTextBackground": "#4C605718",
    "diffEditor.removedTextBackground": "#DF3C3C18",
    "diffEditor.diagonalFill": "#E2DDD1",

    "merge.incomingHeaderBackground": "#004DD61A",
    "merge.currentHeaderBackground": "#00B8B21A",
    "merge.border": "#E2DDD1",

    "editorBracketHighlight.foreground1": "#004DD6",
    "editorBracketHighlight.foreground2": "#00B8B2",
    "editorBracketHighlight.foreground3": "#4C6057",
    "editorBracketHighlight.foreground4": "#D99A00",
    "editorBracketHighlight.foreground5": "#24478F",
    "editorBracketHighlight.foreground6": "#206A4B",

    "gitDecoration.addedResourceForeground": "#4C6057",
    "gitDecoration.modifiedResourceForeground": "#004DD6",
    "gitDecoration.deletedResourceForeground": "#DF3C3C",
    "gitDecoration.untrackedResourceForeground": "#00B8B2",
    "gitDecoration.ignoredResourceForeground": "#9AA1A6",

    "terminal.background": "#FCFAF6",
    "terminal.foreground": "#1A1A1A",
    "terminalCursor.foreground": "#004DD6",
    "terminal.ansiBlack": "#111111",
    "terminal.ansiRed": "#DF3C3C",
    "terminal.ansiGreen": "#4C6057",
    "terminal.ansiYellow": "#D99A00",
    "terminal.ansiBlue": "#004DD6",
    "terminal.ansiMagenta": "#7A3EC7",
    "terminal.ansiCyan": "#00B8B2",
    "terminal.ansiWhite": "#EAE7DF",
    "terminal.ansiBrightBlack": "#3B3F45",
    "terminal.ansiBrightRed": "#FF5555",
    "terminal.ansiBrightGreen": "#00CC99",
    "terminal.ansiBrightYellow": "#FFD700",
    "terminal.ansiBrightBlue": "#2E6FE1",
    "terminal.ansiBrightMagenta": "#9B59D0",
    "terminal.ansiBrightCyan": "#24CBC3",
    "terminal.ansiBrightWhite": "#FFFFFF"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "fontStyle": "italic", "foreground": "#556B60" }
    },
    {
      "name": "Comments: Documentation",
      "scope": ["comment.documentation", "comment.block.documentation"],
      "settings": { "foreground": "#4C6057" }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": { "foreground": "#7A1D16" }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": { "foreground": "#555555" }
    },

    {
      "name": "Keywords",
      "scope": ["keyword", "storage"],
      "settings": { "foreground": "#DF3C3C", "fontStyle": "bold" }
    },
    {
      "name": "Types",
      "scope": ["storage.type", "support.type"],
      "settings": { "foreground": "#24478F", "fontStyle": "bold" }
    },
    {
      "name": "Constants",
      "scope": ["constant.language", "support.constant", "variable.language"],
      "settings": { "foreground": "#D99A00" }
    },
    {
      "name": "Variables",
      "scope": ["variable", "support.variable"],
      "settings": { "foreground": "#1A1A1A" }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#004DD6" }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": { "foreground": "#24478F", "fontStyle": "bold" }
    },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": { "foreground": "#7A1D16" }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.character", "constant"],
      "settings": { "foreground": "#D99A00" }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": { "foreground": "#4C6057" }
    },
    {
      "name": "Regex",
      "scope": "string.regexp",
      "settings": { "foreground": "#004DD6" }
    },
    {
      "name": "Symbols",
      "scope": "constant.other.symbol",
      "settings": { "foreground": "#D99A00" }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": { "foreground": "#555555" }
    },
    {
      "name": "HTML: Tags",
      "scope": ["meta.tag"],
      "settings": { "foreground": "#004DD6" }
    },
    {
      "name": "HTML: Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#00B8B2", "fontStyle": "italic" }
    },
    {
      "name": "CSS: Property Names",
      "scope": ["support.type.property-name"],
      "settings": { "foreground": "#D99A00" }
    },
    {
      "name": "CSS: Values",
      "scope": ["support.constant.property-value"],
      "settings": { "foreground": "#4C6057" }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading",
      "settings": { "foreground": "#DF3C3C", "fontStyle": "bold" }
    },
    {
      "name": "Markup: Link",
      "scope": "meta.link",
      "settings": { "foreground": "#004DD6" }
    },
    {
      "name": "Markup: Insertion",
      "scope": "markup.inserted",
      "settings": { "foreground": "#4C6057" }
    },
    {
      "name": "Markup: Deletion",
      "scope": "markup.deleted",
      "settings": { "foreground": "#DF3C3C" }
    },
    {
      "name": "Markup: Changed",
      "scope": "markup.changed",
      "settings": { "foreground": "#24478F" }
    }
  ],
  "semanticTokenColors": {
    "namespace": "#24478F",
    "class": { "foreground": "#24478F", "bold": true },
    "interface": { "foreground": "#24478F", "bold": true },
    "enum": { "foreground": "#D99A00", "bold": true },
    "enumMember": "#D99A00",
    "typeParameter": "#24478F",
    "parameter": "#1A1A1A",
    "property": "#1A1A1A",
    "variable.readonly": "#D99A00",
    "function": { "foreground": "#004DD6" },
    "method": "#004DD6",
    "macro": "#DF3C3C",
    "event": "#DF3C3C",
    "decorator": "#00B8B2",
    "regexp": "#004DD6",
    "string": "#4C6057",
    "number": "#D99A00",
    "comment": "#556B60",
    "operator": "#555555",
    "keyword": { "foreground": "#DF3C3C", "bold": true },
    "modifier": "#DF3C3C",
    "deprecated": { "strikethrough": true }
  }
}
